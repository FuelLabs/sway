#[test]
fn math_u8_overflow_add() {
    let _ = disable_panic_on_overflow();

    let a = u8::max();
    let b = a + 1;

    require(b == 0_u8, b);

    let c = a + 2;

    require(c == 1_u8, c);

    let d = a + u8::max();

    require(d == u8::max() - 1, d);

    let e = a + (u8::max() - 1);

    require(e == u8::max() - 2, e);
}

#[test]
fn math_u16_overflow_add() {
    let _ = disable_panic_on_overflow();

    let a: u16 = u16::max();
    let b: u16 = a + 1;

    require(b == 0_u16, b);

    let c = a + 2;

    require(c == 1_u16, c);

    let d = a + u16::max();

    require(d == u16::max() - 1, d);

    let e = a + (u16::max() - 1);

    require(e == u16::max() - 2, e);
}

