    fn cmp(a !1: b256, b !2: b256) -> bool {
        entry:
        v0 = asm(lhs: a, rhs: b, sz, res) -> res, !3 {
            addi   sz zero i32, !4
            meq    res lhs rhs sz, !5
        }
        ret bool v0
    }

    fn main() -> bool {
        local ptr b256 a

        entry:
        v0 = const b256 0x0202020202020202020202020202020202020202020202020202020202020202, !6
