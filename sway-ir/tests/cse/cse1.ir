// regex: ID=[[:alpha:]0-9]+
// regex: VAR=v\d+v\d+

script {
    fn main() -> bool {
        entry():
        v0 = const u64 11
        v1 = const u64 0
        // check: $(v3=$VAR) = add
        v3 = add v0, v1
        // check: $(v4=$VAR) = add
        v4 = add v0, v1
        v10 = const u64 10
        v11 = add v1, v0
        // check: cmp eq $v3 $v3
        v2 = cmp eq v3 v4
        v3 = cmp eq v11 v3
        v4 = cmp eq v2 v3
        ret bool v4
    }
}
