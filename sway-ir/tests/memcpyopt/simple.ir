script {
    entry fn main() -> u64 {
        local u64 a
        local u64 b

        entry():
        v0 = get_local ptr u64, a
        v1 = const u64 11
        store v1 to v0
        v2 = get_local ptr u64, b
        v3 = load v0
        store v3 to v2
        v4 = load v2
        ret u64 v4
    }
}

// regex: VAL=v\d+
// regex: ID=[[:alpha:]0-9_]+

// check: $(a_ptr=$VAL) = get_local ptr u64, $ID
// check: $(elev=$VAL) = const u64 11
// check: store $elev to $a_ptr

// check: $(b_ptr=$VAL) = get_local ptr u64, $ID
// not: store
// check: mem_copy_val $b_ptr, $a_ptr
