script {
    entry fn main() -> u256 {
        local u256 x
        local u256 y
        local u256 z

        entry():
        v0 = get_local ptr u256, x
        v1 = get_local ptr u256, y
        v2 = get_local ptr u256, z

        mem_copy_val v2, v0
        wide not v1 to v2
        v3 = load v2

        ret u256 v3
    }
}

// check: v3 = load v2
