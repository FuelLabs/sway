script script {
    fn not(v: bool) -> bool {
        entry:
        cbr v, block0, block1

        block0:
        v0 = const bool false
        br block2

        block1:
        v1 = const bool true
        br block2

        block2:
        v2 = phi(block0: v0, block1: v1)
        ret bool v2
    }

    fn main() -> bool {
        entry:
        v0 = const bool true
        cbr v0, not_block0, not_block1

        not_block0:
        v1 = const bool false
        br not_block2

        not_block1:
        v2 = const bool true
        br not_block2

        not_block2:
        v3 = phi(not_block0: v1, not_block1: v2)
        br block01

        block01:
        v4 = phi(not_block2: v3)
        cbr v4, block1, block0

        block0:
        v5 = phi(block01: v4)
        v6 = const bool false
        cbr v6, not_block03, not_block14

        not_block03:
        v1 = const bool false
        br not_block25

        not_block14:
        v2 = const bool true
        br not_block25

        not_block25:
        v7 = phi(not_block03: v1, not_block14: v2)
        br block2

        block2:
        v8 = phi(not_block25: v7)
        br block1

        block1:
        v9 = phi(block01: v4, block2: v8)
        ret bool v9
    }
}
